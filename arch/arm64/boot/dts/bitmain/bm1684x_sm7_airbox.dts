#include "bm1684x.dtsi"
#include "bm1684x_mm_16g.dtsi"
#include "bm1684x_evb.dtsi"

/ {
	/delete-node/ pka@58008000;
	/delete-node/ clp890@50018000;
	info {
		file-name = "bm1684x_airbox.dtb";
	};
};

/ {
	pwmfan: pwm-fan {
		compatible = "pwm-fan";
		pinctrl-names = "default";
		pinctrl-0 = <&pwm0_acquire>;
		pwms = <&pwm 0 40000>; // period_ns
		pwm-names = "pwm0";
		pwm_inuse = "pwm0";
		#cooling-cells = <2>;
		cooling-levels = <0 153 204 230 254>;  //total 255
	};
};

&soc_pwmfan_trip1 {
	temperature = <30000>; /* millicelsius */
};

&soc_pwmfan_trip2 {
	temperature = <60000>; /* millicelsius */
};

&soc_pwmfan_trip3 {
	temperature = <70000>; /* millicelsius */
};

&soc_pwmfan_trip4 {
	temperature = <75000>; /* millicelsius */
};

&vpu_reserved {
	reg = <0x3 0x40000000 0x0 0x08000000>; // 128M, for VPU firmware
	no-map;
};

&ion_for_vpp {
	compatible = "vpp-region";
	reg = <0x4 0x1194d800 0x0 0xee6b2800>; // 3.725G
};

&ion_for_vpu {
	compatible = "vpu-region";
	reg = <0x3 0x48000000 0x0 0xb7ff0000>; // 2.875G
};

&ion_for_npu {
	compatible = "npu-region";
	reg = <0x1 0x24100000 0x1 0xdbf00000>; // 7.436Gg
};

&clock_init {
	clock-rates = \
		<2300000000>, <950000000>, <800000000>,

		<1000000000>, <1000000000>,
		<2000000000>, <50000000>,
		<100000000>, <100000000>,
		<125000000>, <50000000>,
		<25000000>, <125000000>,
		<50000000>, <25000000>,
		<500000000>, <25000000>,
		<1000000000>, <500000000>,
		<800000000>, <100000000>,
		<250000000>, <250000000>,
		<500000000>, <500000000>,
		<50000000>, <50000000>,
		<50000000>, <50000000>,
		<50000000>, <50000000>,
		<50000000>, <50000000>,
		<100000>, <100000>, <100000>,

		<1000000001>,
		<800000000>, <800000001>,
		<2300000001>, <575000000>;
		};

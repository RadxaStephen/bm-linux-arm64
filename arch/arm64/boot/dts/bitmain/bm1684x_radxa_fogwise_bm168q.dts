#include "bm1684x_radxa_aicore.dtsi"

/ {
	model = "Radxa Fogwise BM168Q";
	/delete-node/ pka@58008000;
	/delete-node/ clp890@50018000;
	info {
		file-name = "bm1684x_radxa_fogwise_bm168q.dts";
	};
};

&uart2 {
	status = "disabled";
};

&soc_tpuclk440m_trip {
	temperature = <65000>;
};

&soc_tpuclk75m_trip {
	temperature = <95000>;
};

&soc_crit_trip {
	temperature = <105000>;
};

&vpu_reserved {
	reg = <0x3 0x40000000 0x0 0x08000000>; // 128M, for VPU firmware
	no-map;
};

&ion_for_vpu {
	compatible = "vpu-region";
	reg = <0x3 0x48000000 0x0 0xb7ff0000>; // 2.875G
};

&ion_for_npu {
	compatible = "npu-region";
	reg = <0x1 0x24100000 0x0 0x96e00000>; // 2.5g
};

&clock_init {
	clock-rates = \
		<2300000000>, <950000000>, <800000000>,

		<1000000000>, <1000000000>,
		<2000000000>, <50000000>,
		<100000000>, <100000000>,
		<125000000>, <50000000>,
		<25000000>, <125000000>,
		<50000000>, <25000000>,
		<500000000>, <25000000>,
		<1000000000>, <500000000>,
		<800000000>, <100000000>,
		<250000000>, <250000000>,
		<500000000>, <500000000>,
		<50000000>, <50000000>,
		<50000000>, <50000000>,
		<50000000>, <50000000>,
		<50000000>, <50000000>,
		<100000>, <100000>, <100000>,

		<1000000001>,
		<800000000>, <800000001>,
		<2300000001>, <575000000>;
};
